# DONT NEED TRIM
$> export HOLA=" bonjour hey "
$> echo wesh"$HOLA" | cat -e


# NEED TRIM
$> export HOLA=bonjour
$> export HOLA=" hola et $HOLA"
$> echo $HOLA


